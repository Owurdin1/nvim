    "Doxygen documentation": {
        "prefix": "doxy",
        "description": "Doxygen documentation block.",
        "body": [
            "/**",
            "    $0",
            "*/"
        ]
    },
    "Include DBG_pub.h": {
        "prefix": "dbg_include",
        "description": "Include DBG_pub.h",
        "body": [
            "#define DBG_MODULE \"${TM_FILENAME_BASE/(.+?)_.+$/${1:/upcase}/}\"",
            "#define DBG_TAG \"${TM_FILENAME_BASE/.+?_(.+)$/$1/}\"",
            "#include \"DBG_pub.h\""
        ]
    },
    "Function (Doxygen)": {
        "prefix": "funcd",
        "description": "Function with Doxygen documentation.",
        "body": [
            "/**",
            "    ",
            "*/",
            "${1:return} ${2:symbol}",
            "    (",
            "    $0",
            "    )",
            "{",
            "",
            "} // ${2:symbol}()"
        ]
    },
    "Function (Garmin)": {
        "prefix": "funcg",
        "description": "Function with Garmin documentation.",
        "body": [
            "/*********************************************************************",
            "*",
            "*   PROCEDURE NAME:",
            "*       ${2:symbol}",
            "*",
            "*   DESCRIPTION:",
            "*",
            "*",
            "*********************************************************************/",
            "${1:return} ${2:symbol}",
            "    (",
            "    $0",
            "    )",
            "{",
            "",
            "} /* ${2:symbol}() */"
        ]
    },
